    <validationRules>
        <fullName>PayToAll_vs_IndividualPayout</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Pay_All_Team_Members__c , CONTAINS( TEXT(Team_Payment_Method__c ) , &apos;Individual&apos;) )</errorConditionFormula>
        <errorMessage>Can&apos;t have pay to all with Individual Payout</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PaymentMethod_vs_Schedule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( CONTAINS( TEXT(Team_Payment_Method__c ) , &apos;Commission Year&apos;) , ISPICKVAL( Commission_Attainment_Schedule__r.Calculation_Type__c , &apos;Step&apos;) )</errorConditionFormula>
        <errorMessage>If Schedule Calculation Type = Step, You can Not Pick Commission Year* Payment Methods</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_vs_PayToAll</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Pay_All_Team_Members__c , !Team__c )</errorConditionFormula>
        <errorMessage>You not have Pay To All Member True with Team equals FALSE.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_vs_Schedule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Team__c ,  ISPICKVAL( Team_Payment_Method__c , &apos;Individual Contribution&apos;) , ISPICKVAL( Commission_Attainment_Schedule__r.Calculation_Type__c , &apos;Step&apos;) )</errorConditionFormula>
        <errorMessage>The selected schedule configuration is not compatible with current Team configuration.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_vs_Total_Team_Contribution</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!Team__c ,  CONTAINS( TEXT(Team_Payment_Method__c ) , &apos;Total Team&apos;)  )</errorConditionFormula>
        <errorMessage>You can not have Payment Method of Total Team Contribution with team equals false.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
