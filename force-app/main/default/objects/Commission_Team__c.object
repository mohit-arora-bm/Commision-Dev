                    <default>false</default>
                    <label>Commission Year Individual Period Payout Tier</label>
                </value>
                <value>
                    <fullName>Commission Year Individual Period Payout Step</fullName>
                    <default>false</default>
                    <label>Commission Year Individual Period Payout Step</label>
                </value>
                <value>
                    <fullName>Commission Year Total Team Period Payout Tier</fullName>
                    <default>false</default>
                    <label>Commission Year Total Team Period Payout Tier</label>
                </value>
                <value>
