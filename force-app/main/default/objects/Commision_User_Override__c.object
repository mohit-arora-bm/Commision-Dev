    </nameField>
    <pluralLabel>Commission User Overrides</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Atleast_One</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK( Commission_Attainment_Schedule_Override__c ),
ISNULL( Commission_Percent_Override__c ),
ISNULL(  Commission_Period_Payout__c )
)</errorConditionFormula>
        <errorMessage>At least one of the override should present</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Single_Style_Override</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !ISNULL(Commission_Period_Payout__c  ) ,!ISNULL(Commission_Percent_Override__c  ) )</errorConditionFormula>
        <errorMessage>Can only have either a period payout or a percent override not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_vs_Schedule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(Commission_Attainment_Schedule_Override__r.Calculation_Type__c, &apos;Step&apos;)  , Commission_Sales_Rep__r.Commission_Team__r.Team__c ,  ISPICKVAL(Commission_Sales_Rep__r.Commission_Team__r.Team_Payment_Method__c, &apos;Individual Contribution&apos;)  )</errorConditionFormula>
        <errorMessage>The selected schedule configuration is not compatible with current Team configuration.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
